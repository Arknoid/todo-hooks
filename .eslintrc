{
  "plugins": [
    // ...
    "react-hooks"
  ],
  "extends": ["react-app", "plugin:prettier/recommended"],
  "rules": {
    "react-hooks/rules-of-hooks": "error",
    "prettier/prettier": [
      "warn",
      {
        "semi": true,
        "singleQuote": true,
        "bracketSpacing": true
      }
    ],
    "no-console": ["warn", { "allow": ["warn", "error"] }]
  }
}
